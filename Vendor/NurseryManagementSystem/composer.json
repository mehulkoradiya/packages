{
  "name": "vendor/nursery-management-system",
  "description": "Laravel 12 Nursery Management System as a reusable package (attendance, fees, notifications, dashboards).",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.2",
    "illuminate/support": "^12.0",
    "illuminate/database": "^12.0",
    "illuminate/notifications": "^12.0",
    "illuminate/queue": "^12.0",
    "illuminate/view": "^12.0",
    "illuminate/routing": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Vendor\\NurseryManagementSystem\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vendor\\NurseryManagementSystem\\NurseryManagementSystemServiceProvider"
      ]
    }
  }
}
